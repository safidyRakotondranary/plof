<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="mg.saplof.plof.model.percistence.Administrateur" table="ADMINISTRATEUR">
      <meta attribute="class-description">
         This class contains the employee detail.
      </meta>
      <id name="id" column="ID_ADMINISTRATEUR">
         <generator class="native"/>
      </id>
      <property name="nom" column="NOM_ADMINISTRATEUR" type="string"/>
      <property name="prenom" column="PRENOM_ADMINISTRATEUR" type="string"/>
      <property name="sexe" column="SEXE_ADMINISTRATEUR" type="string"/>
      <property name="mail" column="MAIL_ADMINISTRATEUR" type="string"/>
      <property name="mdp" column="MDP_ADMINISTRATEUR" type="string"/>
      <property name="photoProfil" column="PHOTO_PROFIL_ADMINISTRATEUR" type="string"/>
   </class>
</hibernate-mapping>
